                            <div class="small-box bg-indigo">
                                <div class="inner">
                                        <h3>Tasks</h3>
                                        <a href="/taskveiw"><button type="button" class="btn btn-primary" style="align-content: center;">Veiw</button></a>
                                        <h3></h3>
                                </div>
